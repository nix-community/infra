      "bsd": {
        "check_interval": 1800,
        "description": "nixos-unstable-small bsd",
        "email_notifications": false,
        "email_override": "",
        "input": [
          {
            "name": "nixpkgs",
            "notify_committers": false,
            "type": "git",
            "value": "https://github.com/NixOS/nixpkgs.git nixos-unstable-small"
          },
          {
            "name": "officialRelease",
            "notify_committers": false,
            "type": "boolean",
            "value": "false"
          },
          {
            "name": "release_source",
            "notify_committers": false,
            "type": "git",
            "value": "https://github.com/nix-community/infra.git master"
          },
          {
            "name": "supportedSystems",
            "notify_committers": false,
            "type": "nix",
            "value": "[ \"x86_64-freebsd\" ]"
          }
        ],
        "keep_evaluations": 1,
        "lifecycle": {
          "ignore_changes": [
            "state"
          ]
        },
        "name": "bsd",
        "nix_expression": {
          "file": "hydra/bsd.nix",
          "input": "release_source"
        },
        "project": "nixpkgs",
        "scheduling_shares": 1000,
        "state": "enabled",
        "type": "legacy",
        "visible": true
